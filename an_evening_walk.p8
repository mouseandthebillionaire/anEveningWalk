--control

local left=true
stride_spacing=0



function walk()
  stride_spacing += 1
		if(btnp(4) and btnp(5)) return

		if (stride_spacing > 15) then
			if(btnp(4) and left) then
				walk_forward()
				left=false
			end
		
			if(btnp(5) and left==false) then
				walk_forward()
				left=true
			end
		end
end

function walk_forward()
	stride_spacing = 0
	camz+=0.3
	if camz>1 then
		camz-=1
		camcnr,camseg=advance(camcnr,camseg)
	end
end
