-- an evenine walk
-- a meditative walking simulator
--	by mouse & the billionaire
--	based on mot's creating a psuedo 3d racer tutorial
--	https://www.lexaloffle.com/bbs/?pid=69343

function _init()
	init_game()
	init_starfield()
	init_trail()
end


function _draw()
	if(running==false) then
		draw_menu()
	else
		cls(1)
		draw_starfield()
		draw_trail()
	end
end

function _update()
	if(running==false) then
		if(btnp(5)) then
			running=true
		end
	else
		walk()
	end
end

